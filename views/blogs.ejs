<%- include("partials/header") -%>
    <% content.forEach(function(blog){ %>
        <h1>
            <%= blog.title %>
        </h1>
        <% let shortPost=blog.post; %>
            <% if (shortPost.length>100) {%>
                <% shortPost=shortPost.substring(0, 100) + "..."; }%>
                    <p>
                        <%= shortPost %><span><a href="/blogs/<%= blog.title %>"> Read More</a></span>
                    </p>
                    <% }); %>
                        <%- include("partials/footer") -%>